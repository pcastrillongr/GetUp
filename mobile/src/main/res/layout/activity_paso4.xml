<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.cristinavilas.getup.CrearAlarma.Paso4"
    android:background="#8fd2ca">>

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="200dp"
        android:layout_height="160dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="111dp"
        android:layout_marginEnd="31dp"
        android:src="@drawable/breakfast" />

    <NumberPicker
        android:id="@+id/numberPicker4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView9"
        android:layout_below="@+id/textView9"
        android:layout_marginStart="46dp"
        android:layout_marginTop="47dp"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="104dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="49dp"
        android:text="Cuanto tiempo tardas en desayunar?"
        android:textColor="@color/common_google_signin_btn_text_light_pressed"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/numberPicker4"
        android:layout_alignEnd="@+id/imageView14"
        android:layout_marginBottom="76dp"
        android:layout_marginEnd="44dp"
        android:text="minutos"
        android:textColor="@color/common_google_signin_btn_text_light_pressed"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView9"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="14dp"
        android:src="@drawable/next" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="150dp"
        android:layout_height="130dp"
        android:layout_alignStart="@+id/numberPicker4"
        android:layout_alignTop="@+id/imageView12"
        android:layout_marginStart="8dp"
        android:layout_marginTop="152dp"
        android:src="@drawable/getupblue" />
</RelativeLayout>

